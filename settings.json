  /*
    If the theme doesn't support semantic tokens, 
    you may need to change this setting from 
    Editor < Semantic Highlighting < "configuredByTheme" to "true":

    Note: Dart & Flutter requires this to be true, 
                      so... most of the formatting is below:
                      not all is tested, and some fields might be unnecessary
                      BUT it works, for the most part :)....
  **/
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "keyword.void": {
        "foreground": "#ffff99",
        "fontStyle": "underline"
      },
      "keyword": {
        "foreground": "#ffd399",
        "fontStyle": "underline"
      },
      "keyword.class": {
        "foreground": "#ffff99",
        "fontStyle": "underline"
      },
      "keyword.operator": {
        "foreground": "#ffff99",
        "fontStyle": "underline"
      },
      "keyword.control": {
        "foreground": "#ffff99",
        "fontStyle": "underline"
      },
      "keyword.operator.new": {
        "foreground": "#C678DD",
        "fontStyle": "underline"
      },
      "keyword.operator.expression": {
        "foreground": "#2F372E",
        "fontStyle": "underline"
      },
      "keyword.operator.logical": {
        "foreground": "#27AE60",
        "fontStyle": "underline"
      },
      "property": {
        "foreground": "#ECD7BC",
        "fontStyle": "bold"
      },
      "property.declaration": {
        "foreground": "#282C34",
        "fontStyle": "italic bold"
      },
      "property.readonly.declaration": {
        "foreground": "#D99058",
        "fontStyle": "strikethrough"
      },
      "property.static": {
        "foreground": "#56B6C2",
        "fontStyle": "italic bold"
      },
      "property.static.declaration": {
        "foreground": "#D99058",
        "fontStyle": "italic bold"
      },
      "property.other": {
        "foreground": "#ECD7BC",
        "fontStyle": "strikethrough bold"
      },
      "property.other.declaration": {
        "foreground": "#ECD7BC",
        "fontStyle": "strikethrough bold"
      },
      "variable.other.property": {
        "foreground": "#75539E",
        "fontStyle": "underline bold strikethrough"
      },
      "comment": {
        "foreground": "#FF3F34",
        "fontStyle": "italic"
      },
      "comment.documentation": {
        "foreground": "#FF3F34",
        "fontStyle": "italic"
      },
    }
  },
